{
  "env": {
    "browser": true,
    "jest": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jest/recommended",
    "plugin:jsx-a11y/strict"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "jest", "jsx-a11y"],
  "rules": {},
  "settings": {
    "react": {
        "version": "detect" 
    }
  }
}
